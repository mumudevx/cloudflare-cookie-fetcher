# Cloudflare Cookie Fetcher Configuration
# Copy this file to .env and update with your settings

# ======================================
# Cloudflare Login Credentials
# ======================================
CLOUDFLARE_USERNAME=your@email.com
CLOUDFLARE_PASSWORD=your_password

# ======================================
# Browser Settings
# ======================================
# Run browser in headless mode (true/false)
HEADLESS=false

# Enable humanized cursor movements (true/false)
HUMANIZE=true

# Browser timeout in milliseconds
TIMEOUT=30000

# ======================================
# Output Settings
# ======================================
# Output filename for cookies
COOKIES_FILENAME=cf-cookies.txt

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# ======================================
# Proxy Settings (Optional)
# ======================================
# HTTP proxy URL (leave empty if not using proxy)
HTTP_PROXY=

# HTTPS proxy URL (leave empty if not using proxy)
HTTPS_PROXY=

# ======================================
# Advanced Settings
# ======================================
# Maximum wait time for Cloudflare challenge (seconds)
CHALLENGE_TIMEOUT=30

# Retry attempts for failed operations
MAX_RETRIES=3

# Wait time between retries (seconds)
RETRY_DELAY=5